<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <record id="report_transaksi_xlsx" model="ir.actions.report">
        <field name="name">Print Excel</field>
        <field name="model">indomaret.transaksi</field>
        <field name="report_type">xlsx</field>
        <field name="report_name">indomaret.report_transaksi_xlsx</field>
