        <field name="name">Print Qr Code</field>
        <field name="model">indomaret.barang</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">indomaret.print_qr_code</field>
        <field name="report_file">indomaret.print_qr_code</field>
        <field name="binding_model_id" ref="model_indomaret_barang"/>
        <field name="binding_type">report</field>
